<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<RootNamespace>LogiwaSharp</RootNamespace>
		<Version>1.2.8</Version>
	</PropertyGroup>
	<PropertyGroup>
		<Description>LogiwaSharp is a C# and .NET library that is used to connect with Logiwa.</Description>
		<Authors>Ajay Kumar</Authors>
		<Copyright>Prime Penguin</Copyright>
		<PackageLicenseUrl>https://raw.githubusercontent.com/PrimePenguin/LogiwaSharp/LICENSE</PackageLicenseUrl>
		<PackageProjectUrl>https://github.com/PrimePenguin/LogiwaSharp</PackageProjectUrl>
		<RepositoryUrl>https://github.com/PrimePenguin/LogiwaSharp</RepositoryUrl>
		<PackageTags>Logiwa,WMS</PackageTags>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageIconUrl>https://raw.githubusercontent.com/PrimePenguin/LogiwaSharp/master/PrimePenguin.png</PackageIconUrl>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Abp" Version="5.12.0" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
	</ItemGroup>

</Project>